{
	"name": "lunar-dark",
	"type": "dark",
	"colors": {
		"activityBar.background": "#000", // this
		"activityBarBadge.background": "#0FA3EC",
		"activityBarBadge.foreground": "#ffffff",
		"badge.background": "#0FA3EC",
		"editor.background": "#000",
		"progressBar.background": "#fd4b00",
		"titleBar.activeBackground": "#000", //this
		"sideBar.background": "#000", //this
		"sideBarSectionHeader.background": "#000", //this
		"dropdown.background": "#293331",
		"menu.background": "#1d2625",
		"list.activeSelectionBackground": "#33403e",
		"list.inactiveSelectionBackground": "#33403e",
		"list.dropBackground": "#475957",
		"list.hoverBackground": "#33403e",
		"list.focusBackground": "#475957",
		"statusBar.background": "#1d1135",
		"statusBar.noFolderBackground": "#4b8b3b",
		"statusBar.debuggingBackground": "#4b8b3b",
		"editorGroupHeader.tabsBackground": "#000",
		"tab.activeBackground": "#33403e",
		"tab.activeBorder": "#293331",
		"tab.inactiveBackground": "#000",
		"button.background": "#293331",
		"editorWidget.background": "#33403e",
		"input.background": "#1f2625",
		"input.border": "#33403e",
		"selection.background": "#63a69844",
		"editor.wordHighlightStrongBackground": "#63a69844",
		"focusBorder": "#293331",
		"editor.lineHighlightBackground": "#262e2d",
		"editor.selectionBackground": "#63a69844",
		"pickerGroup.foreground": "#293331",
		"textLink.foreground": "#293331",
		"editorSuggestWidget.highlightForeground": "#0FA3EC"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFF"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.object-literal.key.js"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#fd5d78"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": ""
			}
		},
		{
			"name": "JSDoc Block",
			"scope": [
				"punctuation.definition.block.tag.jsdoc",
			],
			"settings": {
				"foreground": ""
			}
		},
		{
			"name": "Double Quote",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Curly Brace",
			"scope": [
				"punctuation.definition.bracket.curly.begin.jsdoc",
				"punctuation.definition.bracket.curly.end.jsdoc"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#e67700"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#fd5d78"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#fd5d78"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#fd5d78"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#fd5d78"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#c0eb75"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0FA3EC"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fd5d78"
			}
		}
	]
}